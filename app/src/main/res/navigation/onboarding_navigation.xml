<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/onboarding_navigation"
    app:startDestination="@id/onboardingFragment2">

    <fragment
        android:id="@+id/onboardingFragment2"
        android:name="alura.com.gringotts.view.OnboardingFragment"
        android:label="OnboardingFragment" >
        <action
            android:id="@+id/action_onboardingFragment2_to_criarContaFragment2"
            app:destination="@id/criarContaFragment2" />
        <action
            android:id="@+id/action_onboardingFragment2_to_login_navigation"
            app:destination="@id/login_navigation"
            app:popUpTo="@id/onboardingFragment2"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/criarContaFragment2"
        android:name="alura.com.gringotts.view.CriarContaFragment"
        android:label="fragment_criar_conta"
        tools:layout="@layout/fragment_criar_conta" />
    <include app:graph="@navigation/login_navigation" />
</navigation>